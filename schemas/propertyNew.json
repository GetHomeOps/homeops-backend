{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/propertyNew.json",
  "type": "object",
  "properties": {
    "passport_id": {"type": "string", "maxLength": 255},
    "property_name": {"type": "string", "maxLength": 255},
    "main_photo": {"type": ["string", "null"], "maxLength": 255},
    "tax_id": {"type": "string", "maxLength": 255},
    "county": {"type": "string", "maxLength": 255},
    "address": {"type": "string"},
    "address_line_1": {"type": "string", "maxLength": 255},
    "address_line_2": {"type": "string", "maxLength": 255},
    "city": {"type": "string", "maxLength": 255},
    "state": {"type": "string", "maxLength": 2},
    "zip": {"type": "string", "maxLength": 20},
    "owner_name": {"type": "string", "maxLength": 255},
    "owner_name_2": {"type": "string", "maxLength": 255},
    "owner_city": {"type": "string", "maxLength": 255},
    "occupant_name": {"type": "string", "maxLength": 255},
    "occupant_type": {"type": "string", "maxLength": 50},
    "owner_phone": {"type": "string", "maxLength": 50},
    "phone_to_show": {"type": "string", "maxLength": 50},
    "property_type": {"type": "string", "maxLength": 100},
    "sub_type": {"type": "string", "maxLength": 100},
    "roof_type": {"type": "string", "maxLength": 100},
    "year_built": {"type": "integer"},
    "effective_year_built": {"type": "integer"},
    "effective_year_built_source": {"type": "string", "maxLength": 255},
    "sq_ft_total": {"type": "number"},
    "sq_ft_finished": {"type": "number"},
    "sq_ft_unfinished": {"type": "number"},
    "garage_sq_ft": {"type": "number"},
    "total_dwelling_sq_ft": {"type": "number"},
    "sq_ft_source": {"type": "string", "maxLength": 100},
    "lot_size": {"type": "string", "maxLength": 100},
    "lot_size_source": {"type": "string", "maxLength": 100},
    "lot_dim": {"type": "string", "maxLength": 100},
    "price_per_sq_ft": {"type": "string", "maxLength": 50},
    "total_price_per_sq_ft": {"type": "string", "maxLength": 50},
    "bed_count": {"type": "integer"},
    "bath_count": {"type": "integer"},
    "full_baths": {"type": "integer"},
    "three_quarter_baths": {"type": "integer"},
    "half_baths": {"type": "integer"},
    "number_of_showers": {"type": "integer"},
    "number_of_bathtubs": {"type": "integer"},
    "fireplaces": {"type": "integer"},
    "fireplace_types": {"type": "string", "maxLength": 255},
    "basement": {"type": "string", "maxLength": 255},
    "parking_type": {"type": "string", "maxLength": 255},
    "total_covered_parking": {"type": "integer"},
    "total_uncovered_parking": {"type": "integer"},
    "school_district": {"type": "string", "maxLength": 255},
    "elementary_school": {"type": "string", "maxLength": 255},
    "junior_high_school": {"type": "string", "maxLength": 255},
    "senior_high_school": {"type": "string", "maxLength": 255},
    "school_district_websites": {"type": "string"},
    "list_date": {"type": "string", "format": "date"},
    "expire_date": {"type": "string", "format": "date"}
  },
  "additionalProperties": true,
  "required": ["address", "city", "state", "zip"]
}
