{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "email": {
      "type": "string",
      "maxLength": 255
    },
    "phone": {
      "type": "string",
      "maxLength": 50
    },
    "website": {
      "type": "string",
      "maxLength": 255
    }
  },
  "required": ["name"],
  "additionalProperties": true
}
